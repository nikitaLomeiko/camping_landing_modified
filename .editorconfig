root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = false
indent_style = tab

[*.js]
indent_style = tab

[*.vue]
indent_style = tab

[*.{yml,yaml,neon}]
indent_style = space
indent_size = 4

[{package.json,package-lock.json}]
indent_style = space
indent_size = 2

